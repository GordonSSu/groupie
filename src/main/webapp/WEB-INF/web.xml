<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Project2</display-name>
    <servlet>
        <servlet-name>LoginServlet</servlet-name>
        <servlet-class>csci310.servlets.LoginServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>LoginServlet</servlet-name>
        <url-pattern>/loginServlet</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>RegisterServlet</servlet-name>
        <servlet-class>csci310.servlets.RegisterServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>RegisterServlet</servlet-name>
        <url-pattern>/registerServlet</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>GetUsers</servlet-name>
        <servlet-class>csci310.servlets.GetUsers</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>GetUsers</servlet-name>
        <url-pattern>/getUsers</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>GetBlockedUsers</servlet-name>
        <servlet-class>csci310.servlets.GetBlockedUsers</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>GetBlockedUsers</servlet-name>
        <url-pattern>/getBlockedUsers</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>BlockUser</servlet-name>
        <servlet-class>csci310.servlets.BlockUser</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>BlockUser</servlet-name>
        <url-pattern>/blockUser</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>UnBlockUser</servlet-name>
        <servlet-class>csci310.servlets.UnBlockUser</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>UnBlockUser</servlet-name>
        <url-pattern>/unBlockUser</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>GetInvited</servlet-name>
        <servlet-class>csci310.servlets.GetInvited</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>GetInvited</servlet-name>
        <url-pattern>/GetInvited</url-pattern>
    </servlet-mapping>
</web-app>